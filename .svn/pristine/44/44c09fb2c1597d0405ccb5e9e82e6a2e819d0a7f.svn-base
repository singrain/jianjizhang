/**mbook.wxss**/

.content {
  padding: 0 5px;
}

.content .add {
  height: 30px;
  line-height: 30px;
}

.showbox {
  height: 180px;
  background: url('http://bpic.588ku.com/back_pic/00/11/72/3456398ac1ade27.jpg') no-repeat;
  background-size: 100% 100%;
  color: #fff;
}

.showbox-top {
  text-align: center;
}

.s-t-money {
  font-size: 2rem;
  font-family: 'KaiTi';
  padding: 20px 0 8px;
}

.s-t-tag {
  font-size: 12px;
}

.showbox-down {
  margin-top: 10px;
}

.s-d-item {
  width: 50%;
  float: left;
  text-align: center;
}

.mbox {
}

.mbox-item {
}

.m-t-title {
  background-color: #f8f8f8;
  height: 25px;
  line-height: 25px;
  font-size: 14px;
  color: #959595;
  font-family: 'KaiTi';
}

.m-t-t-left {
  float: left;
  margin-left: 10px;
}

.m-t-t-right {
  float: right;
  margin-right: 10px;
}

.m-list {
  height: 50px;
  line-height: 50px;
  border-bottom: 1px ridge #efefef;
  clear: both;
}

.m-l-left {
  float: left;
  margin-left: 10px;
}

.m-l-timg {
  float: left;
}

.m-l-t-img {
  width: 30px;
  height: 30px;
  margin-top: 10px;
  margin-right: 10px;
}

.m-l-title {
  float: left;
}

.m-l-right {
  float: right;
  margin-right: 10px;
}
