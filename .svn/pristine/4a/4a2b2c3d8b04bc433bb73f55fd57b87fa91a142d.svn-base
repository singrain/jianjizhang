/**index.wxss**/
@import "/svgs.wxss";

.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.usermotto {
  margin-top: 200px;
}

.btnin{
  width: 120px;
  height: 40px;
  line-height: 40px;
}

.icon:after{
  font-family: 'iconfont';
  font-weight: 500;
  font-style: normal;
  content: '&#xe65f;'
}