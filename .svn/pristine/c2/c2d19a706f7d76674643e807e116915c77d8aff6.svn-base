/* pages/mbook/addwater.wxss */
@import "/svgs.wxss";

.iconfont {
  font-size: 30px !important;
}

.nav {
  width: 150px;
  height: 32px;
  margin: 0 auto;
  border: 1px solid #f6c142;
  border-radius: 3px;
  font-size: 14px;
  color: #f6c142;
}

.nav-l {
  float: left;
  width: 49%;
  line-height: 32px;
  text-align: center;
  border-right: 1px solid #f6c142;
}

.nav-r {
  float: left;
  width: 49%;
  line-height: 32px;
  text-align: center;
}

.nav-active {
  background-color: #f6c142;
  color: #fff;
}

.jz-box {
  clear: both;
  height: 60px;
  line-height: 60px;
  border-bottom: 1px solid #f2f2f2;
  padding: 20px 0 0;
  /* margin: 10px; */
}

.jz-title {
  float: left;
  margin-left: 10px;
}

.jz-t-img {
  width: 50px;
  height: 50px;
  line-height: 50px;
  border-radius: 50px;
  background-color: #f6c142;
  text-align: center;
  margin-right: 10px;
  color: #fff;
  float: left;
}

.jz-t-text {
  font-size: 20px;
}

.jz-input-box {
  float: right;
  margin-right: 10px;
}

.jz-input {
  text-align: right;
  height: 70px;
  line-height: 70px;
  color: #444;
}

.jz-item {
  width: 100%;
  clear: both;
}

.jz-item-img {
  /* background-color: #444; */
  width: 20%;
  height: 80px;
  text-align: center;
  float: left;
}

.jz-item-img:hover .jz-imgs {
  background-color: #f6c142;
  color: #fff;
}

.jz-imgs {
  width: 50px;
  height: 50px;
  line-height: 50px;
  margin: 0 auto;
  border-radius: 50px;
  text-align: center;
  margin-bottom: 5px;
  background-color: #f6f6f6;
  color: #6d6d6d;
}

.jz-tag {
  width: 100%;
  font-size: 14px;
  color: #6d6d6d;
}
